{
  "price_extraction_patterns": {
    "first_price_patterns": [
      "(\\d+(?:\\.\\d+)?)",
      "(\\d+\\.\\d+)", 
      "@ (\\d+\\.\\d+)"
    ],
    "second_price_patterns": [
      {
        "pattern": "(\\d+\\.?\\d*)[_\\uFF3F]+(\\d+\\.?\\d*)",
        "group": 2,
        "description": "4220_4224 (underscore or fullwidth underscore)"
      },
      {
        "pattern": "(\\d+\\.?\\d*)\\s*[:\\-]\\s*(\\d+\\.?\\d*)",
        "group": 2, 
        "description": "4220-4224 or 4220:4224"
      },
      {
        "pattern": "\\b\\d+\\.?\\d*///(\\d+\\.?\\d*)",
        "group": 1,
        "description": "Pattern with /// separator"
      },
      {
        "pattern": "@\\d+\\.?\\d*\\s*-\\s*(\\d+\\.?\\d*)",
        "group": 1,
        "description": "At symbol with dash"
      },
      {
        "pattern": "2(?:nd)?\\s+limit\\s*@\\s*(\\d+\\.?\\d*)",
        "group": 1,
        "flags": "IGNORECASE",
        "description": "Second limit at price"
      },
      {
        "pattern": "\\b\\d+\\.?\\d*__+(\\d+\\.?\\d*)",
        "group": 1,
        "description": "Double underscore pattern"
      },
      {
        "pattern": "@\\s*\\d+\\.?\\d*\\s*-\\s*(\\d+\\.?\\d*)",
        "group": 1,
        "description": "At symbol with spaces and dash"
      },
      {
        "pattern": "@\\s*\\d+\\.?\\d*\\s*-\\s*(\\d+\\.?\\d*)|:\\s*\\d+\\.?\\d*\\s*-\\s*(\\d+\\.?\\d*)",
        "group": [1, 2],
        "description": "Complex pattern with at or colon"
      },
      {
        "pattern": "\\b\\d+\\.?\\d*\\s*-\\s*(\\d+\\.?\\d*)",
        "group": 1,
        "description": "Simple dash pattern"
      },
      {
        "pattern": "\\b\\d+\\b\\s*و\\s*(\\d+)\\s*فروش",
        "group": 1,
        "description": "Persian sell pattern"
      },
      {
        "pattern": "\\b\\d+\\b\\s*و\\s*(\\d+)\\s*خرید",
        "group": 1,
        "description": "Persian buy pattern"
      },
      {
        "pattern": "\\b\\d+\\.?\\d*/(\\d+\\.?\\d*)",
        "group": 1,
        "description": "Slash separator pattern"
      },
      {
        "pattern": "=\\s*(\\d+\\.?\\d*)",
        "group": 1,
        "description": "Equals sign pattern"
      },
      {
        "pattern": "(?:\\d+\\.\\d+)[^\\d]+(\\d+\\.\\d+)",
        "group": 1,
        "description": "Two numbers separated by non-digits"
      },
      {
        "pattern": "(\\d+\\.?\\d*)\\D{1,3}(\\d+\\.?\\d*)",
        "group": 2,
        "description": "Last resort: two numbers with 1-3 non-digits between"
      }
    ],
    "tp_patterns": [
      "tp\\s*\\d*[@:.-]?\\s*(\\d+\\.\\d+|\\d+)",
      "tp\\s*(?:\\d*\\s*:\\s*)?(\\d+\\.\\d+)",
      "\\btp\\b\\s*[:\\-@.]?\\s*(\\d+(?:\\.\\d+)?)",
      "tp\\s*:\\s*(\\d+\\.?\\d*)",
      "tp1\\s*:\\s*(\\d+\\.?\\d*)",
      "tp1\\s*\\s*(\\d+\\.?\\d*)",
      "tp\\s*[-:]\\s*(\\d+\\.\\d+|\\d+)",
      "tp\\s*1\\s*[-:]\\s*(\\d+\\.\\d+|\\d+)",
      "checkpoint\\s*1\\s*:\\s*(\\d+\\.?\\d*|OPEN)",
      "takeprofit\\s*1\\s*=\\s*(\\d+\\.\\d+|\\d+)",
      "take\\s*profit\\s*1\\s*:\\s*(\\d+\\.\\d+|\\d+)",
      "tp\\d+\\.\\s*(\\d+\\.?\\d*)",
      "tp\\.\\s*(\\d+\\.?\\d*)",
      "tp\\.(\\d+\\.?\\d*)",
      "تی پی\\s*(\\d+)"
    ],
    "sl_patterns": [
      "sl\\s*:\\s*(\\d+\\.\\d+)",
      "sl\\s*:\\s*(\\d+\\.?\\d*)",
      "(?i)stop\\s*(\\d+\\.?\\d*)",
      "حد\\s*(\\d+\\.\\d+|\\d+)",
      "STOP LOSS\\s*:\\s*(\\d+\\.?\\d*)",
      "sl\\s*[-:]\\s*(\\d+\\.\\d+|\\d+)",
      "sl\\s*[:\\-]\\s*(\\d+\\.?\\d*)",
      "stop\\s*loss\\s*[:\\-]\\s*(\\d+\\.?\\d*)",
      "sl\\s*(\\d+\\.?\\d*)",
      "stop\\s*loss\\s*[@:]\\s*(\\d+\\.?\\d*)",
      "Stoploss\\s*=\\s*(\\d+\\.\\d+|\\d+)",
      "SL\\s*@\\s*(\\d+\\.\\d+|\\d+)",
      "(?i)stop\\s*loss\\s*(\\d+)",
      "استاپ\\s*(\\d+\\.?\\d*)",
      "sl[\\s.:]*([\\d]+\\.?\\d*)",
      "stop\\s*loss\\s*(?:point)?\\s*[:\\-]?\\s*(\\d+\\.\\d+|\\d+)",
      "sl\\s*:::*(\\d+\\.?\\d*)"
    ],
    "simple_price_pattern": "@[\\s]*([0-9]+(?:\\.[0-9]+)?)"
  },
  "description": {
    "first_price_patterns": "Regex patterns to extract primary entry prices",
    "second_price_patterns": "Regex patterns to extract secondary entry prices", 
    "tp_patterns": "Regex patterns to extract take profit levels",
    "sl_patterns": "Regex patterns to extract stop loss levels",
    "simple_price_pattern": "Simple pattern for @ symbol prices"
  }
}